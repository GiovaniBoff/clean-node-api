{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --colors",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --passWithNoTests --findRelatedTests --silent --noStackTrace",
    "test:ci": "npm test -- --coverage",
    "prepare": "npx husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GiovaniBoff/clean-node-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GiovaniBoff/clean-node-api/issues"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "npm run test:staged"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "homepage": "https://github.com/GiovaniBoff/clean-node-api#readme",
  "devDependencies": {
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "standard": "^16.0.3"
  }
}